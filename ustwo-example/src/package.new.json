    "aliasify": "^2.0.0",
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browsernizr": "^2.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "classnames": "^2.2.5",
    "consolidate": "^0.14.1",
    "ellipsize": "0.0.2",
    "es6-promise": "^3.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "he": "^1.1.0",
    "hex-rgb": "^1.0.0",
    "html-webpack-plugin": "^2.21.0",
