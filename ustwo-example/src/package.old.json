    "autoprefixer": "6.0.3",
    "babel": "5.8.21",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "browsernizr": "^1.1.0",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "classnames": "^2.1.5",
    "consolidate": "0.13.1",
    "ellipsize": "0.0.2",
    "es6-promise": "^3.0.2",
    "escodegen": "^1.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "he": "0.5.0",
