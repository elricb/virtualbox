{
  "name": "ustwo-example",
  "version": "1.0.0",
  "private": true,
  "description": "sample react-wordpress VirtualBox",
  "main": "app/index.jsx",
  "scripts": {
    "test": "NODE_ENV=development mocha --compilers js:./src/app/lib/testdom.js,js:babel/register $(find test/unit/ -name '*.js')",
    "integration": "NODE_ENV=development babel-node ./src/app/lib/parallel-mochas '--compilers js:babel/register --reporter spec ./test/integration/*.js'",
    "build": "webpack --progress --colors -p",
    "start": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080",
    "server": "babel-node --debug --presets es2015 -- src/server --debug"
  },
  "keywords": [
    "react",
    "vagrant",
    "virtualbox",
    "webpack"
  ],
